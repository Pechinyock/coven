{
    "server": {
        "address": "localhost",
        "port": 6969,
        "readTimeoutSec": 30,
        "writeTimeoutSec": 30
    },
    "https": {
        "certFilePath": "C:/_dev/coven/cert/localhost+3.pem",
        "certKeyFilePath": "C:/_dev/coven/cert/localhost+3-key.pem"
